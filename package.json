{
  "name": "discourse-graph",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "deploy": "turbo deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "turbo": "^2.5.4",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.15.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./packages/tailwind-config/tailwind.config.ts"
  },
  "dependencies": {
    "posthog-js": "^1.261.8",
    "colord": "^2.9.3"
  }
}
